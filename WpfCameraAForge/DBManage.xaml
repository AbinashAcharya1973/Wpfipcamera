<UserControl x:Class="WpfCameraAForge.DBManage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfCameraAForge"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid HorizontalAlignment="Center">
        <Button Content="Create Database Backup" Width="300" Height="30" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,170,0,0" Click="UpdateButton_Click"/>
        <Button Content="Upload Backup to Server" Width="300" Height="30" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,240,0,0" x:Name="btnUpload" Click="btnUpload_Click"/>
    </Grid>
</UserControl>
